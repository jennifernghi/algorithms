    "def reverse(a):\n",
    "    if len(a) == 1:\n",
    "        return a[0]\n",
    "    elif len(a) == 0:\n",
    "        return a\n",
    "    else:\n",
    "        return reverse(a[1:])+ ' ' + a[0]\n",
    "    \n",
    "str = 'geeks quiz practice code'\n",
    "reverse(str.split())"
